super + h
    bspc node -f west
super + j
    bspc node -f south
super + k
    bspc node -f north
super + l
    bspc node -f east

super + shift + h
    bspc node -s west
super + shift + j
    bspc node -s south
super + shift + k
    bspc node -s north
super + shift + l 
    bspc node -s east

super + alt + h
    bspc node -z left 20 0
super + alt + l
    bspc node -z right 20 0
super + alt + k
    bspc node -z top 0 20
super + alt + j
    bspc node -z bottom 0 20

super + ctrl + l
    bspc node -z left -20 0
super + ctrl + h
    bspc node -z right -20 0
super + ctrl + j
    bspc node -z top 0 -20
super + ctrl + k
    bspc node -z bottom 0 -20


super + d
    dmenu_run
super + f
    python3 ~/passwords/python/search/search.py
super + c
    kitty
super + i
    . ~/passwords/bash/speakText/speakText.sh

super + alt + q 
    bspc quit
super + alt + x 
    systemctl suspend
super + alt + r
    bspc wm -r
super + z
    . ~/.config/sxhkd/macros/startsxhkd.sh
super + s
    bspc node -c


